  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>HttpClientMock</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <WarnOn>3390;$(WarnOn)</WarnOn>
    <AssemblyName>HttpClientMock</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="FuncUtils.fs" />
    <Compile Include="RequestCapture.fs" />
    <Compile Include="MatchResponse.fs" />
    <Compile Include="MockHandler.fs" />
    <Compile Include="MultipartContentProvider.fs" />
    <Compile Include="RequestBehaviorBuilder.fs" />
    <Compile Include="UrlMatchers.fs" />
    <Compile Include="MockedHttpClientBuilder.fs" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>